{
	"name": "agent-memory-mcp",
	"version": "0.1.0",
	"description": "Self-hostable MCP server for AI agent memory with semantic search",
	"type": "module",
	"main": "src/index.ts",
	"scripts": {
		"dev": "wrangler dev",
		"deploy": "wrangler deploy",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:unit": "vitest run test/unit",
		"test:integration": "vitest run test/integration",
		"test:e2e": "vitest run test/e2e",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"typecheck": "tsc --noEmit",
		"cf-typegen": "wrangler types",
		"migrate": "npx tsx scripts/migrate.ts",
		"export": "npx tsx scripts/export.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jonnyparris/agent-memory-mcp.git"
	},
	"keywords": ["mcp", "ai", "memory", "cloudflare", "workers", "semantic-search"],
	"author": "Jonny Parris",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/jonnyparris/agent-memory-mcp/issues"
	},
	"homepage": "https://github.com/jonnyparris/agent-memory-mcp#readme",
	"devDependencies": {
		"@biomejs/biome": "^1.9.0",
		"tsx": "^4.19.0",
		"typescript": "^5.7.0",
		"vitest": "^2.1.0",
		"wrangler": "^4.61.1"
	},
	"dependencies": {
		"@modelcontextprotocol/sdk": "^1.5.0",
		"zod": "^3.24.0"
	}
}
